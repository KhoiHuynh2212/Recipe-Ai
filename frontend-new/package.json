    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:ignore": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:ci": "cross-env CI=false react-scripts build",
    "test": "react-scripts test --watchAll=false --testEnvironment=jsdom",
